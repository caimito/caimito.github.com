---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Caimito Agile Life - Artículos en Español</title>
  <link href="https://www.caimito.net/es/posts.xml" rel="self" />
  <link href="https://www.caimito.net/es/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://www.caimito.net/es</id>
  <author>
    <name>Stephan Schwab</name>
    <email>sns@caimito.net</email>
  </author>
  <logo>https://www.caimito.net/img/Caimito-C-solid.png</logo>
  <icon>https://www.caimito.net/img/Caimito-C-solid.png</icon>
  {% for post in site.posts %}
    {% if post.tags contains 'es' %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="https://www.caimito.net{{ post.url }}" />
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>https://www.caimito.net{{ post.id }}</id>
    <summary>{{ post.excerpt | strip_html | xml_escape }}</summary>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
    {% endif %}
  {% endfor %}
</feed>
